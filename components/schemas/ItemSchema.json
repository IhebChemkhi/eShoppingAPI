{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "title": "selling car",
            "description": "selling a pretty good car ",
            "category": "cars",
            "location": "Oulu",
            "images": "test456.png",
            "price": 20000,
            "type": "BMW amg",
            "shipping": "pickup"
        }
    ],
    "required": [
        "title",
        "description",
        "category",
        "location",
        "images",
        "price",
        "type",
        "shipping"
    ],
    "properties": {
        "title": {
            "$id": "#/properties/title",
            "type": "string",
            "title": "The title schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "selling car"
            ]
        },
        "description": {
            "$id": "#/properties/description",
            "type": "string",
            "title": "The description schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "selling a pretty good car "
            ]
        },
        "category": {
            "$id": "#/properties/category",
            "type": "string",
            "title": "The category schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "cars"
            ]
        },
        "location": {
            "$id": "#/properties/location",
            "type": "string",
            "title": "The location schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Oulu"
            ]
        },
        "images": {
            "$id": "#/properties/images",
            "type": "string",
            "title": "The images schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "test456.png"
            ]
        },
        "price": {
            "$id": "#/properties/price",
            "type": "integer",
            "title": "The price schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                20000
            ]
        },
        "type": {
            "$id": "#/properties/type",
            "type": "string",
            "title": "The type schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "BMW amg"
            ]
        },
        "shipping": {
            "$id": "#/properties/shipping",
            "type": "string",
            "title": "The shipping schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "pickup"
            ]
        }
    },
    "additionalProperties": false
}